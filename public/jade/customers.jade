    body
      // Header
      header
        .container
            .divider
                .logo
                    a(href="./").logo
                        span Talk Push Logo
                    a(href="#").menu-button
                        i(class="fa fa-bars",aria-hidden="true")
                .menu
                    include ./_navigation.jade
      
      .display-header.about
        .filter
            h1.fade Customers
      .white-holder
      
      // Footer Section
      include ./_footer.jade
      
      // Scripts Section
      script(src="./dist/_app.js")
